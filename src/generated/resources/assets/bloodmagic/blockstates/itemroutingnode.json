{
  "multipart": [
    {
      "apply": {
        "model": "bloodmagic:block/routing/modelroutingnodecore"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "bloodmagic:block/routing/modelroutingnodebase"
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "bloodmagic:block/routing/modelroutingnodebase",
        "x": 180
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "bloodmagic:block/routing/modelroutingnodebase",
        "x": 270
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "bloodmagic:block/routing/modelroutingnodebase",
        "x": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "bloodmagic:block/routing/modelroutingnodebase",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "bloodmagic:block/routing/modelroutingnodebase",
        "x": 90,
        "y": 270
      }
    }
  ]
}